{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Ogre Base Project (Debug)",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/dist/bin/OgreBaseProject_d",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}/build/dist/bin",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
      }
    ]
  }